!-mg_coarse_ksp_max_it 25

-mg_k_semi 1
!-mg_i_semi 1
!-mg_j_semi 1
! KSP solver for Poisson equation
-ps_ksp_type fgmres 
!-ps_pc_type hypre
!-ps_pc_hypre_type bommeramg
!-ps_pc_type bjacobi
-ps_ksp_gmres_restart 20
!-ps_ksp_atol 1.e-15
-ps_ksp_rtol 1.e-15
-ps_ksp_max_it 30 
-ps_ksp_truemonitor


!-ps_pc_mg_type full
!-ps_mg_levels_1_ksp_type richardson
!-ps_mg_levels_2_ksp_type richardson
!-ps_mg_levels_3_ksp_type fgmres
!-ps_mg_levels_1_pc_factor_shift_nonzero
!-ps_mg_levels_2_pc_factor_shift_nonzero
