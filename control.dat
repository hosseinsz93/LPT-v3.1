! Intervals for solution output
-tio 200
-imm 1 
! Flow Reynolds number
-ren 25000 
! CFL number
-cfl 0.2
! Nondimensionalized time step 
! dt = Delta t / (L/U)
-dt 0.01
! Non-dimensionalize inflow or not ?
-nondimensionalize_inflow 1 
! Single grid iterations of Poisson equation after mg
-after_mg 0 
-totalsteps 10 
!-rstart 2000 
!-rs_fsi 2200 
! Characteristic length
-chact_leng 25.4
