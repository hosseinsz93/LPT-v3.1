-inflow 1 
-inlet 3 
-k_limit 10
!-init1 
-slice 1 
-twoD 0 
-mhv 0 
-str 1
!-lv 1 
-aneurysm 1
-cfr 0.1 
!Specify type of solver
!0 - RK, 1- NK, 2 - RK_Multigrid
-solver 1 
-imp 3  
-poisson 1 
!--- Setting up for Newton Krylov solver 
!-ksp_monitor 1
!-ksp_max_it 35 
!-- SNES - specification ----------
-snes_mf
-snes_type tr
-snes_max_it 20 
-snes_monitor
-snes_ksp_ew
-snes_ksp_ew_version 3
-snes_atol 1e-8
-snes_rtol 1e-5
-snes_max_funcs 50
-dmmg_jacobian_mf_fd 1
